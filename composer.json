{
    "name": "pkboom/laravel-plaid-webhooks",
    "description": "",
    "keywords": [
        "pkboom",
        "laravel-plaid-webhooks"
    ],
    "homepage": "https://github.com/pkboom/laravel-plaid-webhooks",
    "license": "MIT",
    "authors": [
        {
            "name": "Keunbae Park",
            "email": "ppotpo@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "orchestra/testbench": "^3.8",
        "stripe/stripe-php": "^6.35",
        "symfony/var-dumper": "^4.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Pkboom\\PlaidWebhooks\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pkboom\\PlaidWebhooks\\PlaidWebhooksServiceProvider"
            ],
            "aliases": {
                "PlaidWebhooks": "Pkboom\\PlaidWebhooks\\Facade\\PlaidWebhooks"
            }
        }
    }
}
